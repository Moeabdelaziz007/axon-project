    "test:bridge": "curl -sS http://localhost:3000/api/bridge/api/dashboard | head -c 500",
    "test:bridge:post": "curl -sS -X POST http://localhost:3000/api/bridge/send_message -H 'Content-Type: application/json' -d '{\"message\":\"Hello Quantum-Brain\"}' | head -c 500",
